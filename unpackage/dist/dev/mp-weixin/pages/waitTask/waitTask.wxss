/* 水平间距 */
.v-wait {
  overflow: auto;
}
.v-wait .block {
  margin-bottom: 30rpx;
  width: 100%;
  background-color: #FFFFFF;
}
.v-wait .block .title {
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
}
.v-wait .block .list {
  background-color: #f5f5f5;
}
.v-wait .block .item {
  display: flex;
  justify-content: space-between;
  margin-top: 4rpx;
  padding: 20rpx;
  background-color: #FFFFFF;
}

