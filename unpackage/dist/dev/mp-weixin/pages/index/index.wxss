/* 水平间距 */
/* 水平间距 */
.v-index .navBar {
  width: 100%;
  height: 120rpx;
  line-height: 160rpx;
  color: #FFFFFF;
  background-color: #ff5e6d;
  text-align: center;
}
.v-index .content {
  overflow: auto;
}
.v-index .block {
  box-sizing: border-box;
  width: 95%;
  background-color: #FFFFFF;
  border-radius: 10rpx;
  margin: 20rpx auto;
  padding: 20rpx;
}
.v-index .block .block_title {
  text-align: center;
  font-size: 32rpx;
  margin-bottom: 20rpx;
}
.v-index .block .block_item {
  width: 100%;
  padding: 10rpx;
  margin: 0 auto;
  border-bottom: 4rpx solid #f5f5f5;
}
.v-index .block .item_title {
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
  font-size: 30rpx;
}
.v-index .block .mb-10 {
  margin-bottom: 20rpx;
}

